         <xsd:complexType name="AdditionalCustomerProfileInfoType"> 
		          <xsd:annotation>
			            <xsd:documentation>
			This is an entity to collect all fields that are not in the current CustomerProfile
			core type. At the moment, this is used for the CustomerProfileAdd service.
	      
			Any additional fields should be added here unless it is part of the Core.
			</xsd:documentation>
		          </xsd:annotation>
	           <xsd:sequence>
		             <xsd:element minOccurs="0" name="OverrideFlag" type="xsd:boolean">
			               <xsd:annotation>
				                 <xsd:documentation>
			    Flag to indicate if the customer record has already been overwritten before
			    and if true, then the OverrideBy would be the last user who has overriden
			    the information in the customer's profile.
			    </xsd:documentation>
			               </xsd:annotation>
		             </xsd:element>
		             <xsd:element minOccurs="0" name="OverrideBy" type="xsd:string">
			               <xsd:annotation>
			                  <xsd:documentation>
			    Last person to override the customer profile. Implies that OverrideFlag is TRUE.
			    </xsd:documentation>
			               </xsd:annotation>
		             </xsd:element>
		             <xsd:element minOccurs="0" name="IDTypeOfTax" type="xsd:string">
			               <xsd:annotation>
				                 <xsd:documentation>IT,TN</xsd:documentation>
			               </xsd:annotation>
		             </xsd:element>
		             <xsd:element minOccurs="0" name="Education" type="ecore:CodeNamePairType">
			               <xsd:annotation>
				                 <xsd:documentation>
				1. ต่ำกว่าม.6
