				<xsd:annotation>
					<xsd:documentation>Used  for   Commercial , Business</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="BUHDetail"
				type="eus0:BossProfileDetailType">
			</xsd:element>
			14012016-1-end -->
			<!-- Added as per discussion with BPM, 12/05/2011, and feedback from CRM Team, Wykeen, 16/05/2011-->
			<!-- 14012016-1
			<xsd:element minOccurs="0" name="SMDetail"
				type="eus0:BossProfileDetailType">
			</xsd:element>
			14012016-1-end -->
			<!-- 20120716 Add HoBUDetail -->
			<!-- 14012016-1
			<xsd:element minOccurs="0" name="HoBUDetail"
				type="eus0:BossProfileDetailType">
			</xsd:element>
			14012016-1-end -->
			<!-- 20120716 end -->
			<xsd:element minOccurs="0" name="UserProfileHierarchyDetails"
                            type="eus0:UserProfileHierarchyDetailsType"> <!-- 14012016-2 -->
			</xsd:element>
		          </xsd:sequence>
	        </xsd:complexType>
         <xsd:complexType name="UserProfileHierarchyDetailsType"> <!-- 18012016-1 -->
		<xsd:sequence>
			            <xsd:element maxOccurs="unbounded" minOccurs="1" name="UserProfileHierarchyDetail"
                            type="eus0:UserProfileHierarchyDetailType"/>
		          </xsd:sequence>
	        </xsd:complexType>
         <xsd:complexType name="UserProfileHierarchyDetailType"> <!-- 17022017-1 --> 
	  <xsd:complexContent>
               <xsd:extension base="eus0:BaseUserProfileDetailType">
		                <xsd:sequence>
		                   <xsd:element maxOccurs="1" minOccurs="1" name="AllocationCode"
                                  type="tns:UserAllocationCodeType"/>
		                   <!-- 01032017-1 -->
		    <xsd:element maxOccurs="1" minOccurs="0" name="CustomerAccessCode" type="xsd:string">
		    	                 <xsd:annotation>
		    		                   <xsd:documentation>
		    		Access Code คือ Code ที่จัดระดับสิทธิ์การเข้าถึงข้อมูลลูกค้า ของเจ้าหน้าที่แต่ละ user group
		    		เนื่องจากเจ้าหน้าที่แต่ละสายงานมี user group ที่แตกต่างกัน การกำหนด access code จะทำให้เทียบสิทธิที่เท่าเทียมกันของแต่ละ user group ของแต่ละสายงานได้
		    		</xsd:documentation>
		    	                 </xsd:annotation>
		                   </xsd:element>
                  </xsd:sequence>
		             </xsd:extension>
	           </xsd:complexContent>
	        </xsd:complexType>
      </xsd:schema>
        <xsd:schema xmlns:tns="http://www.bangkokbank.com/schema/entity/BusinessCenter/2011/01/15"
                  attributeFormDefault="unqualified"
                  elementFormDefault="unqualified"
                  targetNamespace="http://www.bangkokbank.com/schema/entity/BusinessCenter/2011/01/15">
         <xsd:complexType name="BusinessCenterType">
		          <xsd:annotation>
			            <xsd:documentation>
		'Code' field contains BC Code from CCIS
		'Name' field contains BC Name from CCIS 
		</xsd:documentation>
		          </xsd:annotation>
		          <xsd:sequence>
			            <xsd:element minOccurs="0" name="BCCode" type="tns:BusinessCenterCodeType">
				              <xsd:annotation>
					                <xsd:documentation>Business center code that this user report to (used  for  Metro-Corporate , Commercial , Business )
    	        </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element minOccurs="0" name="BCNameTH" type="xsd:string">
				              <xsd:annotation>
					                <xsd:documentation>Business Center Name in Thai (Max 70 chars).
Business center Thai name that this user report to.                 
    	        </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element minOccurs="0" name="BCNameEN" type="xsd:string">
				              <xsd:annotation>
					                <xsd:documentation>Business Center Name in English (Max 70 chars).
Business center English name that this user report to.
    	        </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
		          </xsd:sequence>
	        </xsd:complexType>
         <xsd:simpleType name="BusinessCenterCodeType">
		          <xsd:restriction base="xsd:string">
