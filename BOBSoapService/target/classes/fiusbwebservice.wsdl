	        </xsd:complexType>
         <xsd:element name="CcyType" type="ecore:CurrencyTypeType"/>
         <xsd:element name="MsgLines" type="ecore:MsgLinesType"/>
         <xsd:complexType name="NameLineType">  <!-- 06012015-1 -->
      <xsd:sequence>
