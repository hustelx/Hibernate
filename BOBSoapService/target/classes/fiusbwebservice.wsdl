			                  <xsd:documentation>Address Line used if the provider application provides address information
			in a single line or if the address can be concatenated</xsd:documentation>
		                </xsd:annotation>
			            </xsd:element>
			            <!-- 30062014-2 -->
			<xsd:element minOccurs="0" name="AddressLines" type="ecore:AddressLines">
