                  elementFormDefault="unqualified"
                  targetNamespace="http://www.bangkokbank.com/schema/entity/User/2011/01/15">
         <xsd:import namespace="http://www.bangkokbank.com/schema/entity/BusinessCenter/2011/01/15"/>
