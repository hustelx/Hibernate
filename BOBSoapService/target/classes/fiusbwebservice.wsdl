            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="AccountBalanceType">
            <xsd:annotation>
               <xsd:documentation>Represents an Account Balance Entity</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element maxOccurs="1" minOccurs="0" name="AccountName" type="xsd:string"> <!-- 30092015-1 -->
                <xsd:annotation>
                    <xsd:documentation>Deposit  Account Name</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element maxOccurs="1" minOccurs="0" name="AccountProdCode" type="xsd:string"/> 
               <!-- 23112015-1 -->
            <xsd:element maxOccurs="1" minOccurs="0" name="AccountProdDesc" type="xsd:string"> <!-- 08092015-1 -->
                <xsd:annotation>
                    <xsd:documentation>
                    Product Description   of  Deposit account  such as 
					- S/A Saving individual
					- S/A Saving cooperate
					</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element maxOccurs="1" minOccurs="0" name="Branch" type="ebr:BranchCodeNameType"> <!-- 01092015-1 -->
                <xsd:annotation>
                    <xsd:documentation>Deposit Account branch  name</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element maxOccurs="1" minOccurs="0" name="AvailableBalance" type="ecore:CurrAmtType"/> 
               <!-- 08092015-2 -->
            <xsd:element maxOccurs="1" minOccurs="0" name="CurrentBalance" type="ecore:CurrAmtType">
                  <xsd:annotation>
                    <xsd:documentation>Added for IM Accounts that have OD
                </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element maxOccurs="1" minOccurs="0" name="ODLimitAmount" type="ecore:CurrAmtType">
                  <xsd:annotation>
                    <xsd:documentation>Added for IM Accounts that have OD
                </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element maxOccurs="1" minOccurs="0" name="ODExpiryDate" type="ecore:DateType">
                  <xsd:annotation>
                    <xsd:documentation>Added for IM Accounts that have OD. Only Tier 1 expiry date is returned
                </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element maxOccurs="1" minOccurs="0" name="CreditLineAmount" type="ecore:CurrAmtType"> <!-- 29082017-1 -->
                <xsd:annotation>
                    <xsd:documentation>CreditLineAmount for Loan Account</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <!-- 31102012 - 1 -->
            <xsd:element maxOccurs="1" minOccurs="0" name="AccountStatus" type="xsd:string">
            </xsd:element>
               <!-- 20032013-1 -->
            <xsd:element maxOccurs="1" minOccurs="0" name="Owners" type="cus0:OwnersType">
                  <xsd:annotation>
                     <xsd:documentation>Only Name and RMNum be used now</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element minOccurs="0" name="CTCode" type="xsd:string"/> 
               <!-- 12062017-1 -->
            <!-- 25022016-1 -->
            <xsd:element minOccurs="0" name="AccountShortName" type="xsd:string"/> 
               <!-- 01062017-1 -->
            <xsd:element minOccurs="0" name="AccountOpenedDate" type="ecore:DateType"/> 
               <!-- 01062017-1 -->
            <xsd:element minOccurs="0" name="AcctHoldFlag" type="xsd:boolean"> <!-- 28062017-1 -->
            	<xsd:annotation>
            		       <xsd:documentation>Account is hold, that can be Full Hold or Partial Hold.</xsd:documentation>
            	     </xsd:annotation>
               </xsd:element>
               <xsd:element minOccurs="0" name="FullHoldFlag" type="xsd:boolean">
                  <xsd:annotation>
                     <xsd:documentation>
                 Entire balance is set to Hold. i.e. available balance = 0.00.
                 NOTE: Not related whatsoever with the TodayFullHoldAmt or YesterdayFullHoldAmt.
                </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element minOccurs="0" name="StopChequeFlag" type="xsd:boolean"/> 
               <!-- 01062017-1 -->
            <xsd:element minOccurs="0" name="HoldFundingAmt" type="ecore:CurrAmtType">
                  <xsd:annotation>
                     <xsd:documentation>
                 Hold Amt of the Current Account(mostly overdraft), on the Savings linked to the 
                Current Account
                </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element minOccurs="0" name="PartialHoldAmt" type="ecore:CurrAmtType">
                  <xsd:annotation>
                     <xsd:documentation>
                Partial Hold Amt, aka FixedHoldAmt.
                </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element minOccurs="0" name="TodayFullHoldAmt" type="ecore:CurrAmtType">
                  <xsd:annotation>
                     <xsd:documentation>
                  Hold the full available balance as of today
                </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element minOccurs="0" name="YesterdayFullHoldAmt" type="ecore:CurrAmtType">
                  <xsd:annotation>
                     <xsd:documentation>
                  Hold the full available balance as of yesterday
                </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element minOccurs="0" name="LastTransDate" type="ecore:DateType"/> 
               <!-- 01062017-1 -->
            <xsd:element minOccurs="0" name="LastCalDate" type="ecore:DateType"/> 
               <!-- 01062017-1 -->
            <xsd:element minOccurs="0" name="BBLInterestReceived" type="ecore:CurrAmtType"/> 
               <!-- 01062017-1 -->
            <xsd:element minOccurs="0" name="BBLInterestPayable" type="ecore:CurrAmtType"/> 
               <!-- 01062017-1 -->
            <xsd:element minOccurs="0" name="AccountLinkFlag" type="xsd:boolean"/> 
               <!-- 01062017-1 -->
            <xsd:element minOccurs="0" name="AccountLinkNum" type="xsd:string"/> 
               <!-- 01062017-1 -->
            <xsd:element minOccurs="0" name="DateTime" type="ecore:DateTimeType"/> 
               <!-- 01062017-1 -->
            <xsd:element minOccurs="0" name="BookBalance" type="ecore:CurrAmtType"/>
               <xsd:element minOccurs="0" name="InsurancePremium" type="ecore:CurrAmtType">
			               <xsd:annotation>
			                  <xsd:documentation>
			    Only KWAN Bualuang Acct types would have this value populate with values.
			    </xsd:documentation>
			               </xsd:annotation>
			            </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="CommonFacilityInfoType">
		          <xsd:sequence>
			            <xsd:element maxOccurs="1" minOccurs="1" name="AccountNum" type="ecore:AcctNumType">
				              <xsd:annotation>
					                <xsd:documentation>
							TF Number has 23 digits ( TFNo + FacilityID), 
							EQ Number has 16 digits (Credit line Master level)
    				    </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>

			            <xsd:element maxOccurs="1" minOccurs="1" name="ExistingLimit" type="ecore:CurrAmtType">
				              <xsd:annotation>
					                <xsd:documentation>
                        Total Limit Amount (TF), Credit Line Amount(EQ), OD Limit(IM)
    				</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			
			            <xsd:element maxOccurs="1" minOccurs="1" name="Outstanding" type="ecore:CurrAmtType">
				              <xsd:annotation>
					                <xsd:documentation>
                        Use Amount(TF), Summary of Drawdown Amount(EQ)
    				</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>

			            <xsd:element maxOccurs="1" minOccurs="1" name="LoanProduct" type="tns:LoanProductType">
