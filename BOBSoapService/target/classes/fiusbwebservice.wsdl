         <xsd:complexType name="FieldValidationType">
		          <xsd:annotation>
			            <xsd:documentation>
					Used for specifying Field Validation options.
					Type and RegexPatterns are for the future.
					All fields are optional to accomodate the condition where validation is required.
					For example, if MinLenght is not used, then there is no minimum length.
			</xsd:documentation>
		          </xsd:annotation>
		          <xsd:attribute name="MinLength" type="xsd:integer" use="optional"/>
		          <xsd:attribute name="MaxLength" type="xsd:integer" use="optional"/>
		          <xsd:attribute name="Type" type="xsd:string" use="optional"/>
		          <xsd:attribute name="RegexPattern" type="xsd:string" use="optional"/>
	        </xsd:complexType>
         <xsd:simpleType name="emptyElementType">  <!-- 07/02/2012-2 -->
		<xsd:annotation>
			            <xsd:documentation>This type is used for cases where only the element is required</xsd:documentation>
