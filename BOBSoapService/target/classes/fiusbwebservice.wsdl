         <xsd:complexType name="NameValueV2Type">  <!-- 23022016-2 -->
	  <xsd:annotation>
	              <xsd:documentation>
	      V2 type of Name Value that supports
	      elements rather than attributes
	      allow for much more flexibility
	    </xsd:documentation>
	           </xsd:annotation>
	           <xsd:sequence>
	    	         <xsd:element name="Name" type="xsd:string"/>
		             <xsd:element minOccurs="0" name="Value" type="xsd:string"/>
		             <xsd:element minOccurs="0" name="Type" type="xsd:string">
	                 <xsd:annotation>
		                   <xsd:documentation>
		      Follows basic XML valid types without the xsd: prefix, thus valid values are:
		      string
		      date
		      decimal
		      integer
		    </xsd:documentation>
		                </xsd:annotation>
		             </xsd:element>
		
		             <!-- the following fields are used for parsing purposes only -->
		<xsd:element minOccurs="0" name="Length" type="xsd:int">
		                <xsd:annotation>
		                   <xsd:documentation>
		      Used for parsing purposes only. Only valid for string types
		    </xsd:documentation>
		                </xsd:annotation>
		             </xsd:element>
		
		             <xsd:element minOccurs="0" name="DecPlaces" type="xsd:string">
		                <xsd:annotation>
		                   <xsd:documentation>
		      Used for parsing purposes only. Only valid for decimal types
		    </xsd:documentation>
		                </xsd:annotation>
		             </xsd:element>
		             <xsd:element minOccurs="0" name="DateFormat" type="xsd:string">
		                <xsd:annotation>
		                   <xsd:documentation>
		      Used for parsing purposes only. Only valid for DateType formats types
		    </xsd:documentation>
		                </xsd:annotation>
		             </xsd:element>
	           </xsd:sequence>
	        </xsd:complexType>
         <xsd:complexType name="stateTokenType">  <!-- 18032016-1 -->
	    <xsd:sequence>
	              <xsd:element minOccurs="0" name="endFlag" type="xsd:boolean">
	                 <xsd:annotation>
	                    <xsd:documentation>
	            This is not used not but can be set by the client to
	            indiciate that this is the last chunk of requests sent by the client.
	          </xsd:documentation>
	                 </xsd:annotation>
	              </xsd:element>
	              <xsd:element name="stateTokenID" type="xsd:string">
	                 <xsd:annotation>
	                    <xsd:documentation>
	            Similiar to nextKey here.
	          </xsd:documentation>
