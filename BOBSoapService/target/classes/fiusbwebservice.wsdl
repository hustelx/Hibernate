		          </xsd:sequence>
	        </xsd:complexType>
         <xsd:complexType name="LGLevelInfoType">
		          <xsd:annotation>
			            <xsd:documentation>
				LGLevelInfo Type has now been extended to be able to handle other Bill types
				such as PN and CBD ( Type 12 ). Due to impact on changes, LGLevel tag is used for both
			</xsd:documentation>
		          </xsd:annotation>
		          <xsd:sequence>
			            <xsd:element maxOccurs="1" minOccurs="1" name="LGNum" type="ecore:ALNumType">
				              <xsd:annotation>
					                <xsd:documentation>to receive the reference number of LG (from ALS thid fields is 'AL number' at Draw Level).</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			
			            <!-- WS, 22/11/2011, Added LoanProduct to show LGLevelInformation Product Type information as per BPM Request -->
			<xsd:element maxOccurs="1" minOccurs="1" name="LoanProduct" type="tns:LoanProductType">
				              <xsd:annotation>
					                <xsd:documentation>
							To receive Facility type on LG Level Info Product Type. The
							possible values are
							LGBB,LGPB,LGMN,LGMS , etc
							PN and CBD codes are supported as well.
    				    </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <!-- WS, 22/11/2011, -->

			<xsd:element maxOccurs="1" minOccurs="1" name="BeneficiaryName" type="xsd:string"> </xsd:element>
			            <xsd:element maxOccurs="1" minOccurs="1" name="LGAmount" type="ecore:CurrAmtType">
				              <xsd:annotation>
					                <xsd:documentation>For ALS System, all LGs are for THB, i.e. Thai Baht</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <!-- 23112012 -->
			<xsd:element maxOccurs="1" minOccurs="0" name="LGDate" type="ecore:DateType">
				              <xsd:annotation>
					                <xsd:documentation>To receive issue LG date.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element maxOccurs="1" minOccurs="0" name="EffectiveDate" type="ecore:DateType"> </xsd:element>
			            <xsd:element maxOccurs="1" minOccurs="0" name="MaturityDate" type="ecore:DateType"> </xsd:element>
			            <xsd:element maxOccurs="1" minOccurs="0" name="ContractDate" type="ecore:DateType"> </xsd:element> 
               <!-- 28052015-1 -->
			<xsd:element maxOccurs="1" minOccurs="0" name="ContractNum" type="xsd:string"> </xsd:element> 
               <!-- 28052015-2 -->
			<xsd:element maxOccurs="1" minOccurs="0" name="EndCollectionFeeDate" type="ecore:DateType">
				              <xsd:annotation>
					                <xsd:documentation>To receive  the end date of collection Fee .</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element maxOccurs="1" minOccurs="0" name="FeeRate" type="xsd:decimal"/>
			            <xsd:element maxOccurs="1" minOccurs="0" name="OverdueFeeAmount" type="ecore:CurrAmtType">
				              <xsd:annotation>
					                <xsd:documentation>This field contain LG overdue fee  (THB).</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element maxOccurs="1" minOccurs="0" name="AutoDebitIndicator" type="xsd:boolean">
				              <xsd:annotation>
					                <xsd:documentation>This field  contains the flag of Auto debit . The possible value is true/false.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <!-- 23112012 - end -->
			
			<!-- 22112012 - 1 -->
			<xsd:element maxOccurs="1" minOccurs="0" name="DebitAccountNum" type="ecore:AcctNumType">
				              <xsd:annotation>
					                <xsd:documentation>Debit Account Number for the draw</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <!-- 22112012 - 2 -->
			<xsd:element maxOccurs="1" minOccurs="0" name="CommitmentDesc" type="xsd:string">
				              <xsd:annotation>
					                <xsd:documentation>Commitment Description for the Facility</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <!-- 22112012 - end -->
		</xsd:sequence>
	        </xsd:complexType>
         <xsd:element name="AccountDefaultInfo" type="tns:AccountDefaultInfoType"/>
         <xsd:complexType name="AccountDefaultInfoType">
            <xsd:sequence>
               <xsd:element maxOccurs="1" minOccurs="1" name="ApplicationCode" type="xsd:string">
                  <xsd:annotation>
                    <xsd:documentation>The system name of the account such as AL /IM/TF.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element maxOccurs="1" minOccurs="1" name="CustAcctNum" type="xsd:string">
                  <xsd:annotation>
                    <xsd:documentation>To receive the customer Account number for each application system. E.g. ACNum from ALS</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element maxOccurs="1" minOccurs="1" name="AcctNum" type="xsd:string">
                  <xsd:annotation>
                    <xsd:documentation>To receive Account Number . The possible value is AL Number , IM Number , TFC Number .</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element maxOccurs="1" minOccurs="1" name="AcctProductCode" type="xsd:string">
                  <xsd:annotation>
                    <xsd:documentation>To receive the porduct code of Account  from ALS/ IM/ TF  system.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <!-- Added 23/06/2011 as per BPM schema request update, Wykeen -->
            <xsd:element maxOccurs="1" minOccurs="0" name="AcctProductGroup" type="xsd:string">
			</xsd:element>
               <!-- Added 23/06/2011 as per BPM schema request update, Wykeen - end -->
            

            <xsd:element maxOccurs="1" minOccurs="0" name="CreditLineAmount" type="xsd:decimal"> </xsd:element> 
               <!-- 12072011-1 -->
            <xsd:element maxOccurs="1" minOccurs="0" name="Balance" type="xsd:decimal"> <!-- 12072011-1 -->
                <xsd:annotation>
                    <xsd:documentation>To receive the Current balance or Original Loan.
( If the product are O/D, P/N, AVAL ,CBD,LG .
This field is CurrentBalance, if M/L , this field is OriginalLoan )</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element maxOccurs="1" minOccurs="0" name="OverDueDay" type="xsd:integer"> <!-- 12072011-1 -->
                <xsd:annotation>
                    <xsd:documentation>The number of  Day overdue  based on each system.</xsd:documentation> 
                  </xsd:annotation>
               </xsd:element>
               <xsd:element maxOccurs="1" minOccurs="0" name="OpenDate" type="ecore:DateType"> <!-- 12072011-1 -->
                <xsd:annotation>
                    <xsd:documentation>The meaning is effective date , For TF . This
field is TF-Release date . For CBD, AVAL,P/N
This field is Advance date.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element maxOccurs="1" minOccurs="0" name="MaturityDate" type="ecore:DateType"> <!-- 12072011-1 -->
                <xsd:annotation>
                    <xsd:documentation>If the product is TF . This field is TF-Expire date .</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element maxOccurs="1" minOccurs="0" name="TFForeignCurrencyAmount"
                            type="ecore:CurrAmtType">
                  <xsd:annotation>
                    <xsd:documentation>CurrencyCode Field: Three Character Currency Code e.g. THB, USB, etc</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element maxOccurs="1" minOccurs="0" name="TFTerm" type="xsd:integer">
                  <xsd:annotation>
                    <xsd:documentation>Available in TF System only.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element maxOccurs="1" minOccurs="0" name="TFLiability" type="xsd:string">
                  <xsd:annotation>
                    <xsd:documentation>To receive the Liabilty Code. (Available in TF System only.)</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            
            </xsd:sequence>
         </xsd:complexType>
         <xsd:element name="AccountDefaultInfos" type="tns:AccountDefaultInfosType"/>
         <xsd:complexType name="AccountDefaultInfosType">
            <xsd:sequence>
               <xsd:element maxOccurs="unbounded" minOccurs="0" name="AccountDefaultInfo"
