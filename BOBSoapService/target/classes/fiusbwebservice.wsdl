         <xsd:import namespace="http://www.bangkokbank.com/schema/entity/CoreLib/2011/01/15"/>
         <xsd:complexType name="BaseUserProfileDetailType">
		          <xsd:annotation>
			            <xsd:documentation>
          Represents the Basic User Profile Detail Entity. Each User can have multiple User Allocations.
    	</xsd:documentation>
		          </xsd:annotation>
		          <xsd:sequence>
			            <xsd:element maxOccurs="1" minOccurs="1" name="UserID" type="xsd:string">
				              <xsd:annotation>
					                <xsd:documentation>User logon ID ( Network_Id  : BBL standard user name is given to the user by OA administrator; used for network authentication)</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element maxOccurs="1" minOccurs="1" name="StaffID" type="xsd:string">
				              <xsd:annotation>
					                <xsd:documentation>Staff ID that is used for user identifier;</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element maxOccurs="1" minOccurs="1" name="UserNameTH" type="xsd:string">
				              <xsd:annotation>
					                <xsd:documentation>User name in Thai, including title</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element maxOccurs="1" minOccurs="1" name="UserNameEN" type="xsd:string">
				              <xsd:annotation>
					                <xsd:documentation>User name in English, including title</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element minOccurs="1" name="UserEmail" type="ecl0:EmailAddrType">
				              <xsd:annotation>
					                <xsd:documentation>Email address</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element minOccurs="0" name="UserRoles" type="eus0:UserRolesType"> <!-- 14012016-2 -->
				<xsd:annotation>
					                <xsd:documentation>User roles for this user, for example RO, RM in the case of
					  Loan division or other roles depending on the context.
					  From the User Profile, each person can have multiple roles.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
		          </xsd:sequence>
	        </xsd:complexType>
         <xsd:complexType name="AllocationCodePortOwnerType">
		          <xsd:sequence>
			            <xsd:element maxOccurs="1" minOccurs="1" name="AllocationCode"
                            type="eus0:UserAllocationCodeType">
				              <xsd:annotation>
					                <xsd:documentation>RO/ARM Allocation Code </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element maxOccurs="1" minOccurs="0" name="PortOwnerFlag" type="xsd:boolean"> <!-- 03032015-1 -->
				<xsd:annotation>
					                <xsd:documentation>This fields contains Port Owner Flag . If this Allocation Code  is  port owner of  a Customer , the value is true</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
		          </xsd:sequence>
	        </xsd:complexType>
         <xsd:complexType name="UserRolesType">
		          <xsd:sequence>
			            <xsd:element maxOccurs="unbounded" minOccurs="1" name="UserRole" type="ecl0:CodeDescType"/>
		          </xsd:sequence>
	        </xsd:complexType>
         <xsd:complexType name="UserProfileDetailType">
		          <xsd:complexContent>
			            <xsd:extension base="eus0:BaseUserProfileDetailType">
				              <xsd:sequence>
					                <xsd:element maxOccurs="1" minOccurs="1" name="UserAllocations"
                                  type="eus0:UserAllocationsType"/>
				              </xsd:sequence>
			            </xsd:extension>
		          </xsd:complexContent>
	        </xsd:complexType>
         <xsd:simpleType name="UserAllocationCodeType">
		          <xsd:annotation>
			            <xsd:documentation>
          Represents the UserAllocationCodeType. This should be aligned with the physical definition as defined in CCIS
    	</xsd:documentation>
		          </xsd:annotation>
		          <xsd:restriction base="xsd:string">
			            <xsd:maxLength value="13"/>
		          </xsd:restriction>
	        </xsd:simpleType>
         <xsd:element name="UserAllocationCode" type="eus0:UserAllocationCodeType"/>
         <xsd:complexType name="UserAllocationsType">
		          <xsd:annotation>
			            <xsd:documentation>
          Represents the group of UserAllocations for a particular UserId
    	</xsd:documentation>
		          </xsd:annotation>
		          <xsd:sequence>
			            <xsd:element maxOccurs="unbounded" minOccurs="0" name="UserAllocation"
                            type="tns:UserAllocationType"/>
		          </xsd:sequence>
	        </xsd:complexType>
         <xsd:complexType name="UserAllocationType">
		          <xsd:sequence>
			            <xsd:element minOccurs="1" name="AllocationCode" type="eus0:UserAllocationCodeType">
				              <xsd:annotation>
					                <xsd:documentation>Individual allocation code as Business Transformation (BT) definition, assign to this user. AllocationCode(Max 13 chars)
                  
    	        </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element minOccurs="0" name="OfficePhoneNum" type="ecl0:PhoneNumType">
				              <xsd:annotation>
					                <xsd:documentation>Office Telephone no</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element minOccurs="0" name="Building" type="xsd:string">
				              <xsd:annotation>
					                <xsd:documentation>Building name of Office. Building (Max 30 chars)
    	        </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>

			            <xsd:element minOccurs="0" name="BusinessCenter" type="ebc0:BusinessCenterType"> </xsd:element>
			            <xsd:element minOccurs="0" name="Division" type="ediv0:BusinessDivisionType"> </xsd:element>
			            <xsd:element minOccurs="0" name="UserProfileHierarchy" type="eus0:UserProfileHierarchyType"> <!-- 17022017-1 -->
				<xsd:annotation>
					                <xsd:documentation>Boss hierarchy for Approval process </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element minOccurs="0" name="FloorNum" type="xsd:string">
				              <xsd:annotation>
					                <xsd:documentation>
                  Floor Num of Office(Max 2 chars)
    	        </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element minOccurs="1" name="BUCode" type="ecl0:BUCodeType">
				              <xsd:annotation>
					                <xsd:documentation>Business unit code 
    1st Character is 
         N :Normal
         S :Special Asset Management [SAM]  ( Not used)<br/>
                        <br/>
    2nd Character is
         W :Corporate<br/>
         C :Commercial<br/>
         B :Business<br/>
         R :Consumer ( Not used)<br/>  
                        <br/>
   3rd Character is
         M:Metro
         P:Provincial
<br/>
BU Code (Max 3 chars)
    	        </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
		          </xsd:sequence>
	        </xsd:complexType>
         <xsd:complexType name="UserProfileHierarchyType"> <!-- 17022017-1 -->
		<xsd:sequence>
			<!-- 14012016-1
			<xsd:element minOccurs="0" name="RMDetail"
				type="eus0:BossProfileDetailType">
			</xsd:element>
			<xsd:element minOccurs="0" name="ManagerDetail"
