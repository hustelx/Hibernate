         <xsd:complexType name="ChequePaymentInfoType">
	           <xsd:sequence>
	              <xsd:element name="ChequeNum" type="xsd:string"/>
	              <xsd:element minOccurs="0" name="ChequeName" type="xsd:string"/> 
               <!-- 09122016-1 -->
		<xsd:element minOccurs="0" name="ChequeDate" type="tns:DateType"/>
		             <xsd:element minOccurs="0" name="Bank" type="tns:CodeNamePairType"/>
		             <xsd:element minOccurs="0" name="Branch" type="tns:CodeNamePairType"/>
		             <xsd:element minOccurs="0" name="ClearingStatus" type="xsd:string">
