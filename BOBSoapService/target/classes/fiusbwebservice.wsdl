			            <xsd:element maxOccurs="1" minOccurs="1" name="Ctl1" type="xsd:int"/>
			            <xsd:element maxOccurs="1" minOccurs="1" name="Ctl2" type="xsd:int"/>
			            <xsd:element maxOccurs="1" minOccurs="1" name="Ctl3" type="xsd:int"/>
