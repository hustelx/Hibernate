         <xsd:complexType name="OTPVerifyInfoType">
	           <xsd:sequence>
	              <xsd:element name="RefID" type="xsd:string"/>
		             <xsd:element minOccurs="0" ref="ecore:OTPTokenNum"/> 
               <!-- 28042016-2 -->
	  </xsd:sequence>
	        </xsd:complexType>
         <xsd:simpleType name="LengthUnitType">
	           <xsd:annotation>
	              <xsd:documentation>
		  Metric system
	      KM = Kilometre
	      M  = Metre
	      cm = centimetre
	      mm = milimetre
		  
		  Imperial System
		  Mi = Mile
		  Y = Yard
		  F = Feet
		  i = inch
	    </xsd:documentation>
	           </xsd:annotation>
	           <xsd:restriction base="xsd:string">
	              <xsd:enumeration value="KM"/>
	              <xsd:enumeration value="M"/>
	              <xsd:enumeration value="cm"/>
	              <xsd:enumeration value="mm"/>
		             <xsd:enumeration value="Mi"/>
		             <xsd:enumeration value="Y"/>
		             <xsd:enumeration value="F"/>
		             <xsd:enumeration value="i"/>
	           </xsd:restriction>
	        </xsd:simpleType>
         <xsd:simpleType name="EmailAddressType">  <!-- 05012015-1 -->
      <xsd:restriction base="xsd:string"> 
