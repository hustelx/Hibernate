	           <xsd:complexContent>
		             <xsd:extension base="ecore:CodeNamePairType"/>
		          </xsd:complexContent>
	        </xsd:complexType>
         <xsd:complexType name="DateTimeType">
		          <xsd:annotation>
			            <xsd:documentation>
				DateTime type used to encapsulate Date and Time
			</xsd:documentation>
		          </xsd:annotation>
		          <xsd:sequence>
			            <xsd:element minOccurs="0" name="Date" type="tns:DateType"/>
			            <xsd:element minOccurs="0" name="Time" type="xsd:time"/>
