			            <xsd:element minOccurs="0" name="District" type="ecore:DistrictType"> </xsd:element>
			            <xsd:element minOccurs="0" name="Province" type="ecore:ProvinceType"> </xsd:element>
			            <xsd:element minOccurs="0" name="PostCode" type="xsd:string"> </xsd:element>
			            <xsd:element minOccurs="0" name="Country" type="ecore:CountryType"> </xsd:element>
			            <xsd:element minOccurs="0" name="AddressLine" type="xsd:string">    <!-- 19/08/2011-1 -->
