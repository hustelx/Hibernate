					                <xsd:documentation>
							If Production name is "TF Share Limit" then Description is not Blank .  
    				    </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element maxOccurs="1" minOccurs="0" name="ODFacilityInfo" type="tns:ODFacilityInfoType">
				              <xsd:annotation>
					                <xsd:documentation>
							Only available if the facility is for OD type
    				    </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
		          </xsd:sequence>
