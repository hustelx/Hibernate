		             <xsd:element minOccurs="0" name="JobPosition" type="xsd:string"> </xsd:element>
		             <xsd:element minOccurs="0" name="Nationality2" type="xsd:string"> </xsd:element>
		             <xsd:element minOccurs="0" name="Nationality3" type="xsd:string"> </xsd:element>
		             <xsd:element minOccurs="0" name="PlaceOfBirth" type="xsd:string"> </xsd:element>
		             <xsd:element minOccurs="0" name="SubstantialIndicator" type="xsd:boolean"> <!--  16032016-1 -->
			<xsd:annotation>
				                 <xsd:documentation>Y/N – RM store Y/Space</xsd:documentation>
			               </xsd:annotation>
		             </xsd:element>
		             <xsd:element minOccurs="0" name="GreenCardFlag" type="xsd:boolean">
			               <xsd:annotation>
				                 <xsd:documentation>Y/N – RM store Y/Space</xsd:documentation>
			               </xsd:annotation>
		             </xsd:element>
		             <xsd:element minOccurs="0" name="USDeclarationFormFlag" type="xsd:boolean"> <!--  16032016-2a -->
		   <xsd:annotation>
		                   <xsd:documentation>
		      Flag to indicate that Customer has submitted US Declaration, the contents of which
		      is not indicated. i.e. we do not know if the customer has declared that they are a US citizen
		      or declared as not..this is just used to indicate that a form has been submitted.
		    </xsd:documentation>
		                </xsd:annotation>
		             </xsd:element>
		
		             <xsd:element minOccurs="0" name="W9FormFlag" type="xsd:boolean"> <!--  16032016-2b -->
		  <xsd:annotation>
		                   <xsd:documentation>
		      Flag to indicate if W9 Form has been submitted by the Customer
		    </xsd:documentation>
		                </xsd:annotation>
		             </xsd:element>
		
		             <xsd:element minOccurs="0" name="StandingInstructionFlag" type="xsd:boolean">
		                <xsd:annotation>
		                   <xsd:documentation>
		      Flag to indicate if Customer has a Standing Instruction
		    </xsd:documentation>
		                </xsd:annotation>
		             </xsd:element>
		
		             <xsd:element minOccurs="0" name="Classification" type="ecore:CodeNamePairType">
			               <xsd:annotation>
			                  <xsd:documentation>
			    Classification of the customer. Possible code values are:
			    Blank - Customers without Financial Account (Out of Scope of FATCA)<br/>
				90 - Blank-NUSP (less than or equal to USD 50,000)<br/>
				91 - Blank-USP (less than or equal USD 50,000)<br/>
				92 - S-NUSP (greater than USD 50,000 - 1M without US Indicia Hit)<br/>
				93 - S-USP (greater than USD 50,000 - 1M with US Indicia Hit)<br/>
				94 - HV-NUSP (greater than USD 1 M without US Indicia Hit)<br/>
				95 - HV-S-USP (greater than USD 1M with US Indicia Hit)<br/>
				01 – NUSP<br/>
				02 – USP<br/>
				03 - Recalcitrant-NUSP<br/> 
				04 - Recalcitrant-USP<br/>
			                  </xsd:documentation>
			               </xsd:annotation>
